<h1>Sign up!</h1>

<%= render 'shared/errors' %>

<form action="<%= users_url %>" method="post">
  <%= hidden_auth_token %>

  <label for="user_email">Your email address</label>
  <input id="user_email"
         type="text"
         name="user[email]"
         value="<%= @user.email %>">

   <label for="user_password">Password</label>
   <input id="user_password"
          type="password"
          name="user[password]">

  <button>Sign up!</button>
</form>

<% if request.status == "PENDING" %>
  <form method="post" action="<%= deny_cat_rental_request_url(request) %>">
    <input type="hidden" name="authenticity_token"
      value="<%= form_authenticity_token %>">
    <button>DENY</button>
  </form>
  <form method="post" action="<%= approve_cat_rental_request_url(request) %>">
    <input type="hidden" name="authenticity_token"
      value="<%= form_authenticity_token %>">
    <button>APPROVE</button>
  </form>
<% end %>
